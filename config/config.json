{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "fluenti-ui-controls-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/fluentiUiControls/FluentiUiControlsWebPart.js",
          "manifest": "./src/webparts/fluentiUiControls/FluentiUiControlsWebPart.manifest.json"
        }
      ]
    },
    "sample-form-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/sampleForm/SampleFormWebPart.js",
          "manifest": "./src/webparts/sampleForm/SampleFormWebPart.manifest.json"
        }
      ]
    },
    "file-upload-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/fileUpload/FileUploadWebPart.js",
          "manifest": "./src/webparts/fileUpload/FileUploadWebPart.manifest.json"
        }
      ]
    },
    "handling-large-list-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/handlingLargeList/HandlingLargeListWebPart.js",
          "manifest": "./src/webparts/handlingLargeList/HandlingLargeListWebPart.manifest.json"
        }
      ]
    },
    "spfx-crud-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/spfxCrud/SpfxCrudWebPart.js",
          "manifest": "./src/webparts/spfxCrud/SpfxCrudWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "FluentiUiControlsWebPartStrings": "lib/webparts/fluentiUiControls/loc/{locale}.js",
    "SampleFormWebPartStrings": "lib/webparts/sampleForm/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "FileUploadWebPartStrings": "lib/webparts/fileUpload/loc/{locale}.js",
    "HandlingLargeListWebPartStrings": "lib/webparts/handlingLargeList/loc/{locale}.js",
    "SpfxCrudWebPartStrings": "lib/webparts/spfxCrud/loc/{locale}.js"
  }
}
